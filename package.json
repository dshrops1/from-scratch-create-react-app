{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "my create-react-app boilerplate",
  "main": "index.js",
  "scripts": {
    "build": "npm run build-server && npm run build-client",
    "build-server": "babel src/server --out-dir ./dist",
    "build-client": "babel src/client --copy-files --out-dir ./dist/public",
    "start": "node ./dist/index.js"
    "test": "jest ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dshrops1/from-scratch-create-react-app.git"
  },
  "keywords": [
    "[\"Node\"",
    "\"React\"]"
  ],
  "author": "Dustin Shropshire",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dshrops1/from-scratch-create-react-app/issues"
  },
  "homepage": "https://github.com/dshrops1/from-scratch-create-react-app#readme",
  "dependencies": {
    "express": "^4.16.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "jest": "^22.4.3"
  }
}
